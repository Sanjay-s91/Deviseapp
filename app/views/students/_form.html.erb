<%= form_with(model: @student,remote: true) do |form| %>
 

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :roll_no %>
    <%= form.number_field :roll_no %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
